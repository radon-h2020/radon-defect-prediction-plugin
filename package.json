{
	"name": "radon-defect-prediction-plugin",
	"displayName": "Radon-h2020 Defect Predictor",
	"description": "A plugin to run the RADONh2020 Defect Predictor",
	"version": "0.0.1",
	"publisher": "sdallapalma",
	"engines": {
		"vscode": "^1.44.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:radon-defect-prediction-plugin.run"
	],
	"main": "./out/extension.js",
	"contributes": {
		"menus": {
			"editor/title": [
			  {
				"when": "resourceLangId == yaml",
				"command": "radon-defect-prediction-plugin.run",
				"group": "navigation"
			  }
			]
		},
		"commands": [
			{
			  "command": "radon-defect-prediction-plugin.run",
			  "title": "Run detection"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.44.0",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"eslint": "^6.8.0",
		"@typescript-eslint/parser": "^2.26.0",
		"@typescript-eslint/eslint-plugin": "^2.26.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.1",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0",
		"axios": "0.19.2"
	}
}
